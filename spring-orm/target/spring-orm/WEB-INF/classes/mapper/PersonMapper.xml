<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.myspring.orm.ibatis.mapper.PersonMapper">

    <insert id="insertPerson" parameterType="com.myspring.orm.ibatis.bo.Person" flushCache="true">
        INSERT INTO PERSON (id, p_name, p_age) VALUES (#{id}, #{name}, #{age})
    </insert>

</mapper>